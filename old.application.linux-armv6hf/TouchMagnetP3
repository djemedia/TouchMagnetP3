#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
$APPDIR/java/bin/java -Djna.nosys=true -Djava.ext.dirs="$APPDIR/java/lib/ext" -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/apa102-java-rpi-lib-1.0-all.jar:$APPDIR/lib/simutils.jar:$APPDIR/lib/apa102-java-rpi-examples-1.0-all.jar:$APPDIR/lib/toxiclibscore.jar:$APPDIR/lib/colorutils.jar:$APPDIR/lib/TouchMagnetP3.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-natives-linux-armv6hf.jar:$APPDIR/lib/nrserial.jar:$APPDIR/lib/PixelPusher.jar:$APPDIR/lib/apiviz-1.3.1.GA.jar:$APPDIR/lib/umlgraph5.2.jar:$APPDIR/lib/args4j-2.0.8.jar:$APPDIR/lib/toxiclibs.jar:$APPDIR/lib/doapamine.jar:$APPDIR/lib/mp3spi1.9.5.jar:$APPDIR/lib/jl1.0.1.jar:$APPDIR/lib/tritonus_aos.jar:$APPDIR/lib/jsminim.jar:$APPDIR/lib/tritonus_share.jar:$APPDIR/lib/minim.jar:$APPDIR/lib/artnetP5.jar:$APPDIR/lib/dmxP512.jar:$APPDIR/lib/net.jar:$APPDIR/lib/jssc.jar:$APPDIR/lib/serial.jar:$APPDIR/lib/oscP5.jar" TouchMagnetP3 "$@"
